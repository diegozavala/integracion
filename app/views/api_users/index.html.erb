<h1>Listing api_users</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Password</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @api_users.each do |api_user| %>
      <tr>
        <td><%= api_user.name %></td>
        <td><%= api_user.password %></td>
        <td><%= link_to 'Show', api_user %></td>
        <td><%= link_to 'Edit', edit_api_user_path(api_user) %></td>
        <td><%= link_to 'Destroy', api_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api user', new_api_user_path %>
